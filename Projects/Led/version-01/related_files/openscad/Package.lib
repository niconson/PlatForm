include <Package_CUBE.lib>
include <Package_Package.lib>


/* ___________________
  |                   |
  |       PARTS       |
  |___________________|
*/
module Pcb_Package (frozen)
{
  translate([frozen?-originX_Package:0, frozen?-originY_Package:0, 0])
  {
    if( is_undef(drw_Package_Package) )
      Draw_Package_Package();
    else if( drw_Package_Package != 0 )
      Draw_Package_Package();
    if( is_undef(drw_board_outline) )
      Draw_BO_Package(1);
    else if( drw_board_outline != 0 )
      Draw_BO_Package(0);
  }
}


/* ___________________
  |                   |
  |   BOARD OUTLINE   |
  |___________________|
*/
module Draw_BO_Package (bAll=0)
{
}


/* ___________________
  |                   |
  |      MODULES      |
  |___________________|
*/
module Draw_Package_Package ()
{
    translate([ 0.000, 0.000, 0.0 ])
      rotate([ 0.0, 0.0, -180.000 ])
        FPackage_Package();
}
